public void insertLast(int val){
        Node node = new Node(val);
        Node last = head;

        node.next = null;
        if(head == null){
            node.prev = null;
            head = node;
            return;
        }

        while(last.next != null){
            last = last.next;
        }
        last.next = node;
        node.prev = last;
    }
